{
	"presets": [
		["env", {
			"modules": false,
			"targets": {
				"node": "current",
				"browsers": "> 5%"
			},
			"useBuiltIns": true
		}],
		"stage-2"
	],
	"plugins": [
		"ramda",
		"tailcall-optimization"
	],
	"env": {
		"test": {
			"plugins": [
				"ramda",
				"tailcall-optimization",
				["istanbul", {
					"exclude": [
						"**/*.spec.js",
						"tests.webpack.js"
					]
				}]
			]
		}
	}
}